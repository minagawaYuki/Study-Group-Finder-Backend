            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ProductDto": {
        "required": [
          "Name",
          "Price",
          "SellerID"
        ],
        "type": "object",
        "properties": {
          "Name": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "Price": {
            "type": "number",
            "format": "float"
          },
          "SellerID": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "RegisterRequest": {
        "type": "object",
